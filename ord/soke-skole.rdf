         xmlns:skos="http://www.w3.org/2004/02/skos/core#">
   <rdf:Description rdf:about="https://psi.norge.no/los/ord/soke-skole">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="https://psi.norge.no/los/ontologi/ord"/>
      <skos:prefLabel xml:lang="en">Applying to schools</skos:prefLabel>
      <skos:prefLabel xml:lang="nb">Søke skole</skos:prefLabel>
      <skos:prefLabel xml:lang="nn">Søkje skule</skos:prefLabel>
      <skos:note xml:lang="nb">Ungdom som har fullført grunnskolen eller tilsvarende opplæring, har rett til tre års videregående opplæring. Søknad til videregående skole gjøres sentralt og det er lik frist over hele landet.</skos:note>
      <skos:note xml:lang="nn">Ungdom som har fullført grunnskulen eller tilsvarande opplæring, har rett til vidaregående opplæring. Søknad til vidaregående skule blir gjord sentralt og det er lik frist over heile landet.</skos:note>
      <skos:broader rdf:resource="https://psi.norge.no/los/tema/videregaende-skole"/>
   </rdf:Description>
</rdf:RDF>
